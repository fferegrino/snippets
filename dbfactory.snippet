  <CodeSnippet Format="1.0.0">
    <Header>
      <Title>Valor desde DataReader</Title>
      <Author>Antonio Feregrino Bola침os</Author>
      <Description>Recuperar un valor desde un DataReader</Description>
      <Shortcut>ixval</Shortcut>
    </Header>
    <Snippet>
      <Declarations>
        <Literal>
          <ID>ParameterName</ID>
          <Default>Parametro</Default>
        </Literal>
        <Literal>
          <ID>ColName</ID>
          <Default>COLUMNA</Default>
        </Literal>
        <Literal>
          <ID>Field</ID>
          <Default>data</Default>
        </Literal>
        <Literal>
          <ID>DataType</ID>
          <Default>String</Default>
        </Literal>
      </Declarations>
      <Code Language="CSharp">
        <![CDATA[int $ParameterName$Ix = reader.GetOrdinal("$ColName$");
            if (!reader.IsDBNull($ParameterName$Ix))
            {
                $Field$ = reader.Get$DataType$($ParameterName$Ix);
            }]]>
      </Code>
    </Snippet>
  </CodeSnippet>
